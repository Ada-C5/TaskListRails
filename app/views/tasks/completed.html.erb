	<h2>Completed Tasks</h2>
  <table>
    <tr class="header-row">

      <th><h3>Title</h3></th>
      <th><h3>Description</h3></th>
      <th><h3>Delete</h3></th>

    </tr>
    <tr class="table-row"><% @task_list.each do |task| %></tr>
    <td><%= task.title %></td>
    <td><%= task.description %></td>
      <td class="delete">
    <%= link_to delete_path(task.id), method: :delete, data: {confirm: 'Are you sure you want to delete this task?'} do %>
        <i class="fa fa-trash" aria-hidden="true"></i>
    <% end %>
      </td>
    <% end %>

  </table>

